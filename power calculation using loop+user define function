#include<stdio.h>
void calculatePower(double base,double exp)
{
    double result=1,i;
    for(i=1;i<=exp;i++)
    {
        result=result*base;
    }
    printf("Power is:%.2lf\n",result);
}
int main()
{
    double base,exp;
    printf("Enter base:\n");
    scanf("%lf",&base);
    printf("Enter exp:\n");
    scanf("%lf",&exp);

    calculatePower(base,exp);
}
or----------------------------------------------------------------------------------------------------------------
#include<stdio.h>
void calculatePower(double base,double exp)
{
    double result=1,i;
    for(i=1;i<=exp;i++)
    {
        result=result*base;
    }
    printf("Power is:%.2lf\n",result);
}
int main()
{
   
    calculatePower(2,3);
    calculatePower(3,3);
    calculatePower(4,3);
}
