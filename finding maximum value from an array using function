#include<stdio.h>
int maximum(int x[])
{
    int i;
    int max=x[0];
    for(i=1;i<6;i++)
    {
        if(max<x[i])
        {
            max=x[i];
        }
    }
    return max;
}
int main()
{
    int num[]={10,200,30,40,50,-5};
    int maximumNumber=maximum(num);
    printf("maximum is:%d\n",maximumNumber);
}
