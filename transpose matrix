#include<stdio.h>
int main()
{
    int A[10][10], transpose[10][10], i,j,row,colum;
    printf("Enter the number of rows and colums of the matrix A:\n");
    scanf("%d %d",&row,&colum);

    for(i=0; i<row; i++)
    {
        for(j=0; j<colum; j++)
        {
            printf("Enter A[%d][%d]=",i,j);
            scanf("%d",&A[i][j]);


        }
        printf("\n");

    }
    for(i=0; i<row; i++)
    {
        for(j=0; j<colum; j++)
        {
            transpose[j][i]=A[i][j];

        }
        printf("\n");

    }
    printf("Matrix A:\n");
     for(i=0; i<row; i++)
    {
        for(j=0; j<colum; j++)
        {
            printf("%d ",A[i][j]);


        }
        printf("\n");

    }
    printf("Transpose Matrix of A:\n");
     for(i=0; i<colum; i++)
    {
        for(j=0; j<row; j++)
        {
            printf("%d ",transpose[i][j]);


        }
        printf("\n");

    }







    getch();
}
