#include<stdio.h>
int main()
{
    typedef char Letter ;
    Letter ch;
    ch='A';
    printf("ch= %c\n",ch); //ch=A
    Letter ch1;
    ch1='B';
    printf("ch1=%c\n",ch1);//ch=B
}
----------------------------------------
#include<stdio.h>
struct book
{
    char name[20];
    int price;
};
int main()
{
    typedef struct book BOOK;  

    BOOK b={"Jiboner kotha",350};

    printf("Book name : %s\n",b.name);
    printf("Book prince : %d",b.price);
    getch();
}          
